{
  "name": "nasa-explorer-backend",
  "version": "1.0.0",
  "description": "Production-ready Node.js/Express backend API for NASA Explorer application with caching, rate limiting, validation, and comprehensive logging",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "echo \"No linter configured yet\"",
    "format": "echo \"No formatter configured yet\""
  },
  "keywords": [
    "nasa",
    "api",
    "mars",
    "space",
    "weather",
    "express",
    "nodejs",
    "caching",
    "rate-limiting",
    "validation",
    "logging"
  ],
  "author": "NASA Explorer Team",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.10.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.2.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": "18.x"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  }
}
